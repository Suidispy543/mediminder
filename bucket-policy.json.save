{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyUnsecureTransport",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::mediminder-prescriptions-prod-mohnish",
        "arn:aws:s3:::mediminder-prescriptions-prod-mohnish/*"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "false"
        }
      }
    }
  ]
}

