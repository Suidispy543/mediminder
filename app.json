{
  "expo": {
    "name": "MediMinder",
    "slug": "mediminder",
    "scheme": "mediminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.mediminder",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "fetch"]
      }
    },
    "android": {
      "package": "com.yourcompany.mediminder",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      ["expo-notifications", { "icon": "./assets/notification-icon.png", "color": "#2563EB" }]
    ],
    "extra": {
      "EXPO_PUBLIC_GEMINI_API_KEY": "AIzaSyCB3mDrc0T302iqZcgnCQxzLXIpj06U6t8"
    }
  }
}
