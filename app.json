{
  "expo": {
    "name": "MediMinder",
    "slug": "mediminder",
    "scheme": "mediminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.mediminder",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.yourcompany.mediminder",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563EB",
          "sounds": [
            "./assets/sounds/sound1.wav",
            "./assets/sounds/sound2.wav"
          ]
        }
      ],
      "expo-router"
    ],
    "extra": {
      "EXPO_PUBLIC_GEMINI_API_KEY": "AIzaSyBjkHUEkpt3jbBrgoGEeYuqfbngJNXwnlQ",
      "EXPO_GEMINI_MODEL": "models/gemini-1.5-flash-8b",
      "HF_API_KEY": "hhf_donkfbmqbOjvisHfHytSRShIYtHsPMSLLa"
    }
  }
}
